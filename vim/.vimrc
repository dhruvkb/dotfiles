" CAUTION: Symlinked file

syntax enable

set number

colorscheme solarized
let g:solarized_termtrans = 1
set background=dark
set guifont=JetBrainsMono\ Nerd\ Font:h14

set whichwrap+=<,>,[,]
set colorcolumn=80

set expandtab
set tabstop=2
set softtabstop=2
set shiftwidth=2

if has('mac') && has('gui_running')
  set macligatures
endif
